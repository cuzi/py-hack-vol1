{{outlet}}
<div class="layout-column flex-xs flex-gt-xs-70">
  {{#paper-card as |card|}}
    {{#card.title as |title|}}
      {{#title.text as |text|}}
        {{#text.headline}} {{model.userName}} {{/text.headline}}
        {{#text.subhead}}{{model.station}}{{/text.subhead}}
        {{#text.subhead}}{{model.channel}}{{/text.subhead}}
      {{/title.text}}
      {{title.media size="lg" src="https://cdn1.iconfinder.com/data/icons/user-pictures/100/male3-512.png" alt="Tomster" title="Tomster"}}
    {{/card.title}}
    {{#card.actions}}
      {{#paper-button  onClick=(action "openModal") }}Edit{{/paper-button}}
      {{#paper-button  onClick=(action "logOut") }}Log Out{{/paper-button}}
    {{/card.actions}}
  {{/paper-card}}
</div>
<div class="layout-column flex-xs flex-gt-xs-70">
  {{#paper-card as |card|}}
    {{#card.title as |title|}}
      {{#title.text as |text|}}
        {{#text.headline}} Request for Help {{/text.headline}}
        {{#paper-form onSubmit=(action "SubmitCall") as |form|}}
  <div class="layout-row">
    <div class="layout-column flex-100">
      {{form.input label="topic" value=firstName onChange=(action (mut firstName)) required=true}}
    </div>
  </div>
  <div class="layout-row">
    <div class="layout-column flex-100">
      {{paper-input
        type="number"
        label="Tier"
        value=maxNumber
        onChange=(action (mut maxNumber))
        min="1"
        max="7"
        errorMessages=(hash
          max="Enter 5 or less."
        )
      }}
    </div>
  </div>
  {{#if showStatusSelect}}
    <div class="layout-row">
        <div class="layout-column flex-100">
          {{#paper-select options=callStatus label="Call Status" required=true selected=pizzaSize onChange=(action (mut pizzaSize)) as |size|}}
            {{size.name}}
          {{/paper-select}}
        </div>
  </div>
  {{/if}}
  <div class="layout-row">
    <div class="layout-column flex-100">
      {{form.input label="CustomerID" value=Cid onChange=(action (mut Cid)) required=false}}
    </div>
  </div>
  <div class="layout-row">
    <div class="layout-column flex-100">
      {{form.input label="Description" value=desc onChange=(action (mut desc)) required=false}}
    </div>
  </div>
  <div class="layout-row">
    <div class="layout-column flex-100">
      {{form.input label="Inccident" value=Inccident onChange=(action (mut Inccident)) required=false}}
    </div>
  </div>
  <div class="layout-row">
    {{#form.submit-button raised=true primary=true}}Submit{{/form.submit-button}}
  </div>
{{/paper-form}}
      {{/title.text}}
    {{/card.title}}
  {{/paper-card}}
  {{#if modalShown}}
  {{#paper-dialog fullscreen=fullscreen onClose=(action "closePromptDialog" "cancel") origin=dialogOrigin}}

    {{#paper-dialog-content}}
    <div class="centeredForm">
      <h2 class="md-title">Edit your Details </h2>
      {{#paper-form onSubmit=(action "edit") as |form|}}
                {{paper-input label="Station Number" type="number" value=model.station onChange=(action (mut model.station)) iconRight="person_pin_circle"}}
            {{#paper-radio-group
            groupValue=(readonly model.channel) class="ChenealValues"
            onChange=(action (mut model.channel)) as |group|}}
              {{#group.radio value="1" primary=true class="radioCenter"}} Chat {{/group.radio}}
              {{#group.radio value="2" warn=true class="radioCenter"}} Social Network {{/group.radio}}
              {{#group.radio value="3" class="radioCenter"}} Email {{/group.radio}}
              {{#group.radio class='lastinput radioCenter' value="4" success=true }} Phone {{/group.radio}}
            {{/paper-radio-group}}
          {{/paper-form}}
      </div>
    {{/paper-dialog-content}}

    {{#paper-dialog-actions class="layout-row"}}
      <span class="flex"></span>
      {{#paper-button primary=true onClick=(action "closePromptDialog" "cancel")}}MY BAD{{/paper-button}}
      {{#paper-button primary=true onClick=(action "closePromptDialog" "ok" dogName)}}DONE{{/paper-button}}
    {{/paper-dialog-actions}}

  {{/paper-dialog}}
{{/if}}

</div>
